{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "TweenLite": false
  },
  "ecmaFeatures": {
    "modules": true
  },
  "extends": "standard",
  "rules": {
    "no-trailing-spaces": [2, {"skipBlankLines": true}],
    "eol-last": 0
  }
}